  gcd_iter_cb [label="gcd_iter\r  0: 1_while_cond:\l  1:     if     a # b goto 2_while_body\l  2:     goto   0\l  3: 2_while_body:\l  4:     if     a > b goto 5_if_true\l  5:     goto   6_if_false\l  6: 5_if_true:\l  7:     sub    t0 <- a, b\l  8:     assign a <- t0\l  9:     goto   4_while_next\l 10: 6_if_false:\l 11:     sub    t1 <- b, a\l 12:     assign b <- t1\l 13: 4_while_next:\l 14:     goto   1_while_cond\l 15: 0:\l 16:     return a\l",shape=box]
